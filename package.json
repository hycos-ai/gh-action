{
  "name": "build-log-analyzer-action",
  "version": "1.0.0",
  "description": "GitHub Action to upload build logs to S3 and analyze them",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build dist/index.js -o dist --source-map --license licenses.txt",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/build-log-analyzer-action.git"
  },
  "keywords": [
    "github-action",
    "logs",
    "s3",
    "aws",
    "analysis",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/lib-storage": "^3.456.0",
    "axios": "^1.6.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.50.0",
    "eslint-plugin-github": "^4.10.0",
    "eslint-plugin-jest": "^27.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=20"
  }
} 